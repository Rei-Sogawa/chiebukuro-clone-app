<div class="static-pages-home">
  <div class="link-to-new-question">
    <h3>
      <%= link_to  "Post a New Question!", new_question_path %>
    </h3>
  </div>
  <div class="resolved-questions">
    <div class="link-to-questions">
      <h3>
        <%= link_to  "See Resolved Questions", questions_path(is_resolved: 1) %>
      </h3>
    </div>
  </div>
  <div class="unresolved-questions">
    <div class="link-to-questions">
      <h3>
        <%= link_to  "See Unresolved Questions", questions_path(is_resolved: 0) %>
      </h3>
    </div>
  </div>
</div>
