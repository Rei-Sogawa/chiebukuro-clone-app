<h3>Unresolved Questions</h3>
<ul class="list-group">
  <%= render partial: "question_title", collection: @unresolved_questions, as: :question %>
  <li class="list-group-item clearfix">
    <div class="pull-right">
      <%= link_to "See more", questions_path(is_resolved: "false") %>
    </div>
  </li>
</ul>
